{
       "version": 2,
       "framework": null,
       "builds": [
         {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": "src/**"
      }
    }
       ],
       "routes": [
         {
           "src": "/(.*)",
           "dest": "src/index.ts"       
         }
       ],
       "outputDirectory": "dist",
       "functions": {
    "src/index.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
     }